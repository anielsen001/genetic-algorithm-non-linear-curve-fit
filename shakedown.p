program shakedown;	uses		gene, avgdata;	var		testgene1, testgene2, one: gene;		child1, child2: gene;		loop: integer;		A, Beta, Sigma, Omega: real;		fit: real;begin	showtext;	randseed := tickcount;	for loop := 0 to 3 do		one[loop] := 1;	one[4] := 0;	one[5] := 0;	one[6] := 1;	one[7] := 1;	one[8] := 0;	one[9] := 0;	one[10] := 1;	one[11] := 1;	one[12] := 0;	one[13] := 0;	one[14] := 1;	one[15] := 0;	one[16] := 1;	one[17] := 1;	one[18] := 1;	one[19] := 0;	one[20] := 0;	for loop := 21 to 37 do		one[loop] := 1;	one[38] := 0;	one[39] := 1;	one[40] := 0;	one[41] := 0;	for loop := 42 to 57 do		one[loop] := 1;	for loop := 58 to 62 do		one[loop] := 0;	for loop := 63 to 79 do		one[loop] := 1;	for loop := 79 to 83 do		one[loop] := 0;	writegeneln(one);	translate(one, a, beta, sigma, omega);	writeln(a, beta, sigma, omega);	fit := makegenefitness(one);	writeln(fit);end.